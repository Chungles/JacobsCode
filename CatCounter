public class Main {
	public static void main(String[] args) {
int myCats = 0;
		if ( myCats = 1 ) {

			System.out.println("I only have " + myCats + " Cat. So sad.");
            myCats++;
		} else if {
		    System.out.println("Yay! I have " + myCats + " cats! That's more than one!");
		    myCats++;
		}
		
	}

}
